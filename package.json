    "@vitejs/plugin-vue": "5.0.5",
    "eslint": "9.7.0",
    "eslint-plugin-vue": "9.27.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "8.3.0",
    "vite": "5.3.4",
    "vue-tsc": "2.0.26",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "stylelint": "16.9.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "cspell": "8.14.4",
    "husky": "^9.1.6"
